[Unit]
Description=Gemini Gateway Backend Service
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/www/gemini_gateway/backend
ExecStart=/var/w/gemini_gateway/backend/venv/bin/python -m gunicorn.app.wsgiapp -c gunicorn_conf.py main:app

[Install]
WantedBy=multi-user.target
